<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.controladores.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <TextField GridPane.rowIndex="0" fx:id="txtExistencia" />
    <TextField GridPane.rowIndex="1" fx:id="txtCompra" />
    <Button GridPane.rowIndex="2" text="Comparar" fx:id="btbComparar"  onAction="#realizarComparacion"/>
</GridPane>